      <h1>Zpracování vyúčtování</h1>
      <form class="navbar-form navbar-left" method="post" action="<%= Portal::Build.url(:build,:process) %>" enctype='multipart/form-data'>
        <%= csrf_token_field %>
        <div class="form-group">
          <div class="files">
            <a href="#" onClick="addFile(event);return false;">Přidat možnost nahrání dalšího souboru</a>
            <div class="input-group">
              <span class="input-group-addon" id="basic-addon1">Soubor pro zpracování (file1)</span>
              <input type="file" name="data[]" class="form-control" placeholder="Nahravana data" aria-describedby="basic-addon1">
            </div>
          </div>
        </div>
        <div class="form-group">
          <label class="control-label">Dostupné recepty:</label>
          <select name="recipe" class="form-control">
            <% @soubory.each do |soubor| %>
                <% next if soubor[1].nil? %>
              <option value="<%= soubor[0] %>/<%= soubor[1] %>"><%= soubor[1] %></option>
            <% end %>
          </select>
        </div>
        <div class="form-group">
          <div class="btn">
            <button class="btn btn-default" type="submit" onClick="process(event)">Zpracovat</button>
          </div>
        </div>
      </form>